{
  "name": "xytra bot",
  "description": "Telegram User Tagger Bot",
  "logo": "https://graph.org/file/bae8ca4a24a6902df2e0c.jpg",
  "keywords": ["telegram", "bot", "tagger"],
  "repository": "https://github.com/Xytra-zn/tagger",
  "env": {
    "PYTHONUNBUFFERED": {
      "description": "Ensure Python is running in unbuffered mode",
      "required": true,
      "value": "1"
    },
    "TELEGRAM_API_ID": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true,
      "value": "your-telegram-api-id"
    },
    "TELEGRAM_API_HASH": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true,
      "value": "your-telegram-api-hash"
    },
    "TELEGRAM_BOT_TOKEN": {
      "description": "The token of your Telegram bot",
      "required": true,
      "value": "your-bot-token"
    },
    "MONGODB_URL": {
      "description": "Your MongoDB connection URL",
      "required": true,
      "value": "your-mongodb-url"
    }
  },
  "addons": [
    {
      "plan": "mongodbatlas",
      "options": {
        "creds": "MONGODB_URL"
      }
    }
  ]
}
