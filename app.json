{
  "name": "XYTRA TAGGER",
  "description": "Telegram User Tagger Bot MADE WITH CHAT GPT",
  "keywords": ["telegram", "bot", "tagger"],
  "repository": "https://github.com/Xytra-zn/tagger",
  "stack": "heroku-22",
  "addons": [
    {
      "options": {
        "version": "12"
      },
      "plan": "heroku-postgresql"
    }
  ],
  "env": {
    "TELEGRAM_API_ID": {
      "description": "Your Telegram API ID (from my.telegram.org/apps)",
      "required": true
    },
    "TELEGRAM_API_HASH": {
      "description": "Your Telegram API Hash (from my.telegram.org/apps)",
      "required": true
    },
    "TELEGRAM_BOT_TOKEN": {
      "description": "Your Telegram Bot Token (from @BotFather)",
      "required": true
    },
    "TELETHON_SESSION": {
      "description": "Your Telethon Session String",
      "required": true
    }
    // Add more environment variables as needed
  },
  "scripts": {
    "postdeploy": "python heroku.py"
  }
}
